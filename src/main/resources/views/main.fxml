<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="600.0" minWidth="800.0" prefHeight="600.0"
            prefWidth="900.0" styleClass="background" stylesheets="@style.css" xmlns="http://javafx.com/javafx/null"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.penblog.filewizard.controllers.AppController">
    <children>
        <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0"
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab text="Rename">
                    <content>
                        <AnchorPane styleClass="background">
                            <fx:include fx:id="renameTab" source="rename/rename.fxml" AnchorPane.bottomAnchor="0.0"
                                        AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                        AnchorPane.topAnchor="0.0"/>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Move">
                    <content>
                        <AnchorPane styleClass="background">
                            <fx:include fx:id="moveTab" source="move/move.fxml" AnchorPane.bottomAnchor="0.0"
                                        AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                        AnchorPane.topAnchor="0.0"/>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <Hyperlink fx:id="hlDownload" text="New update available" AnchorPane.bottomAnchor="10.0"
                   AnchorPane.leftAnchor="15.0"/>
    </children>
</AnchorPane>
